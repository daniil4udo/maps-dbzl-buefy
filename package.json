{
  "name": "maps-dubizle-buefy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --port=8080",
    "dev:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode=development",
    "build": "bash -c 'eval \"VUE_APP_BASE_PATH=$1 vue-cli-service build\"' -- ",
    "build:ci": "yarn build --report",
    "lint": "run-s lint:all:*",
    "lint:all:eslint": "vue-cli-service lint",
    "lint:all:markdownlint": "yarn lint:markdownlint \"{src,public}/**/*.md\" \"*.md\"",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss,css}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "lint:putout": "putout --fix",
    "test": "run-s test:unit test:e2e",
    "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
    "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
    "test:ci": "run-s test:unit:ci test:e2e",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "commit": "git-cz",
    "generate:aliases": "find ./templates -type f -name '*.aliases.json' -exec rm {} + && node aliases.config.js",
    "generate:inspect": "vue-cli-service inspect > output.js && eslint --print-config ./.eslintrc.js > .eslintrc.json"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.21",
    "buefy": "^0.9.4",
    "core-js": "^3.6.5",
    "lodash": "^4.17.20",
    "proper-url-join": "^2.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.40.5",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.165",
    "@types/proper-url-join": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "markdownlint-cli": "^0.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "stylelint-selector-no-empty": "^1.0.7",
    "stylelint-webpack-plugin": "^2.1.1",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
